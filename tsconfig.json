{
  "extends": "./nodedevpkg/vuedevconfig/tsconfig.json",
  "compilerOptions": {
    "noErrorTruncation": true,
    "baseUrl": ".",
    "lib": [
      "dom"
    ],
    "paths": {
      "@webapp/vuekit/*": [
        "./webapp/vuekit/*"
      ],
      "@innoai-tech/vuekit/*": [
        "./nodepkg/vuekit/src/*.ts"
      ],
      "@innoai-tech/vuekit": [
        "./nodepkg/vuekit/src/index.ts"
      ],
      "@innoai-tech/typedef": [
        "./nodepkg/typedef/src/index.ts"
      ],
      "@innoai-tech/*": [
        "nodedevpkg/*/src/index.ts",
        "nodepkg/*/src/index.ts",
        "nodepkg/*/index.d.ts",
        "*"
      ]
    }
  },
  "exclude": [
    "nodepkg/*/*.d.ts"
  ]
}
