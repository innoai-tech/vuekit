{
  "name": "@innoai-tech/vueuikit",
  "version": "0.11.0",
  "monobundle": {
    "exports": {
      ".": "./src/index.ts"
    }
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/serialize": "^1.1.3",
    "@emotion/utils": "^1.2.1",
    "@innoai-tech/lodash": "0.2.1",
    "@innoai-tech/vuekit": "workspace:^",
    "@innoai-tech/csstype": "workspace:^",
    "@material/material-color-utilities": "^0.2.7",
    "@popperjs/core": "^2.11.8",
    "polished": "^4.3.1",
    "popmotion": "^11.0.5",
    "vue": "v3.4.19"
  },
  "peerDependencies": {},
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    }
  },
  "files": [
    "*.mjs",
    "*.d.ts",
    "src/*",
    "!/**/__tests__"
  ],
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:innoai-tech/vuekit.git",
    "directory": "nodepkg/vueuikit"
  },
  "scripts": {
    "lint": "bunx --bun @biomejs/biome check --apply .",
    "build": "bunx --bun monobundle",
    "prepublishOnly": "bun run build"
  },
  "type": "module"
}
