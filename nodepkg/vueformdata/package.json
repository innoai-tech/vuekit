{
  "name": "@innoai-tech/vueformdata",
  "version": "0.1.8",
  "monobundle": {
    "exports": {
      ".": "./src/index.ts"
    }
  },
  "dependencies": {
    "@innoai-tech/lodash": "^0.2.1",
    "@innoai-tech/vuekit": "workspace:^",
    "rxjs": "^7.8.1"
  },
  "peerDependencies": {},
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    }
  },
  "files": [
    "*.mjs",
    "*.d.ts",
    "src/*",
    "!/**/__tests__"
  ],
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:innoai-tech/vuekit.git",
    "directory": "nodepkg/vueformdata"
  },
  "scripts": {
    "lint": "bunx --bun @biomejs/biome check --apply .",
    "build": "bunx --bun monobundle",
    "prepublishOnly": "bun run build"
  },
  "type": "module"
}
