{
  "name": "@innoai-tech/typedef",
  "version": "0.4.8",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:innoai-tech/vuekit.git",
    "directory": "nodepkg/typedef"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": {
        "types": "./src/index.ts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "!/**/__tests__",
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "build": "bunx --bun @innoai-tech/monobundle",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "immer": "^11.1.3",
    "reflect-metadata": "^0.2.2"
  },
  "peerDependencies": {
    "es-toolkit": "^1.43.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "monobundle": {
    "exports": {
      ".": "./src/index.ts"
    }
  }
}
