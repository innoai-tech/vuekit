{
  "name": "@innoai-tech/gents",
  "version": "0.9.1",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:innoai-tech/vuekit.git",
    "directory": "nodepkg/gents"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": {
        "types": "./src/index.ts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "!/**/__tests__",
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "build": "bunx --bun @innoai-tech/monobundle",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@innoai-tech/typedef": "workspace:^",
    "es-toolkit": "^1.44.0"
  },
  "devDependencies": {
    "@innoai-tech/fetcher": "^0.7.3"
  },
  "peerDependencies": {
    "@innoai-tech/config": "*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "monobundle": {
    "build": {
      "clean": true
    },
    "exports": {
      ".": "./src/index.ts"
    }
  }
}
