{
  "name": "@innoai-tech/vuekit",
  "version": "0.6.9",
  "monobundle": {
    "exports": {
      ".": "./src/index.ts",
      "./jsx-runtime": "./src/jsx-runtime.ts",
      "./jsx-dev-runtime": "./src/jsx-dev-runtime.ts"
    }
  },
  "dependencies": {
    "@innoai-tech/lodash": "^0.2.1",
    "@innoai-tech/typedef": "workspace:^",
    "@innoai-tech/vue-jsx-runtime": "workspace:^",
    "immer": "^10.0.3",
    "rxjs": "^7.8.1",
    "vue": "v3.4.15",
    "vue-router": "^4.2.5"
  },
  "peerDependencies": {},
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    },
    "./jsx-runtime": {
      "bun": "./src/jsx-runtime.ts",
      "import": {
        "types": "./jsx-runtime.d.ts",
        "default": "./jsx-runtime.mjs"
      }
    },
    "./jsx-dev-runtime": {
      "bun": "./src/jsx-dev-runtime.ts",
      "import": {
        "types": "./jsx-dev-runtime.d.ts",
        "default": "./jsx-dev-runtime.mjs"
      }
    }
  },
  "files": [
    "*.mjs",
    "*.d.ts",
    "src/*",
    "!/**/__tests__"
  ],
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:innoai-tech/vuekit.git",
    "directory": "nodepkg/vuekit"
  },
  "scripts": {
    "test": "bun test .",
    "lint": "bunx --bun @biomejs/biome check --apply .",
    "build": "bunx --bun monobundle",
    "prepublishOnly": "bun run build"
  },
  "type": "module"
}
