{
  "name": "@innoai-tech/vue-vite-presets",
  "version": "0.14.0",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:innoai-tech/vuekit.git",
    "directory": "nodedevpkg/vue-vite-presets"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": {
        "types": "./src/index.ts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "!/**/__tests__",
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "build": "bunx --bun @innoai-tech/monobundle",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@innoai-tech/purebundle": "^0.8.1",
    "@innoai-tech/vuecomponentcompleter": "^0.5.1",
    "@mapbox/rehype-prism": "^0.9.0",
    "@mdx-js/rollup": "^3.1.1",
    "@swc/core": "1.15.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "es-toolkit": "^1.43.0",
    "globby": "^16.1.0",
    "hastscript": "^9.0.1",
    "rolldown": "^1.0.0-beta.57",
    "unist-util-visit": "^5.0.0",
    "vite": "^8.0.0-beta.0",
    "vite-plugin-pages": "^0.33.2"
  },
  "peerDependencies": {},
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "monobundle": {
    "exports": {
      ".": "./src/index.ts"
    }
  }
}
