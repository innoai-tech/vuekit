    },
    "exports": {
      ".": "./src/index.ts"
    }
  },
  "dependencies": {
    "@innoai-tech/lodash": "^0.2.1",
    "@innoai-tech/purebundle": "^0.2.12",
    "@mapbox/rehype-prism": "^0.9.0",
    "@mdx-js/rollup": "^3.0.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "hastscript": "^8.0.0",
    "unist-util-visit": "^5.0.0",
    "vite": "^5.0.11",
    "vite-plugin-pages": "^0.31.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "peerDependencies": {},
  "exports": {
