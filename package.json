    "es-toolkit": "^1.41.0",
    "normalize.css": "^8.0.1",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "rolldown": "^1.0.0-beta.47",
    "rolldown-vite": "^7.2.2",
    "turbo": "^2.6.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.8",
    "vue": "^3.5.24"
  },
  "resolutions": {
    "rolldown": "^1.0.0-beta.47",
    "rolldown-vite": "^7.2.2",
    "vite": "npm:rolldown-vite@^7.2.2",
    "vue": "^3.5.24"
  },
  "prettier": {
    "plugins": [
      "@prettier/plugin-oxc"
    ]
